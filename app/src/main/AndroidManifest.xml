<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.itdais.materialdesignding"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".bottomsheet.BottomSheetActivity">
        </activity>
        <activity android:name=".drawerlayout.DrawerLayoutMainActivity">
        </activity>
        <activity
            android:name=".drawerlayout.DrawerLayout_BelowToolbarActivity"
            android:label="@string/title_activity_drawer_layout_below"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".drawerlayout.DrawerLayout_OneActivity"
            android:label="@string/title_activity_drawer_layout_one"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".drawerlayout.DrawerLayout_OtherActivity"
                  android:label="@string/title_activity_drawer_layout_other"
                  android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>